<?xml version="1.0" encoding="utf-8"?>
<manifest
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	package="com.yoga.kalkulasi"
	android:versionCode="1"
	android:versionName="1.0">

	<uses-sdk
		android:minSdkVersion="15"
		android:targetSdkVersion="25"/>

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">

		<activity
			android:name=".MainActivity">

			<intent-filter>

				<action
					android:name="android.intent.action.MAIN"/>

				<category
					android:name="android.intent.category.LAUNCHER"/>

			</intent-filter>

		</activity>

		<activity
			android:name=".RegisterActivity"/>

		<activity
			android:name=".LoginActivity"/>

		<activity
			android:name=".KalkulasiAction"/>

		<activity
			android:name=".KalkulasiView"/>

		<activity
			android:name=".SubAction"/>

		<activity
			android:name=".SubView"/>

		<activity
			android:name=".ToolAction"/>

		<activity
			android:name=".ToolView"/>

		<activity
			android:name=".VariabelAction"/>

		<activity
			android:name=".ProjectAction"/>

		<activity
			android:name=".ProjectView"/>

		<activity
			android:name=".KategoriActivity"/>

		<activity
			android:name=".FragmentEmptyTool"/>

		<activity
			android:name=".TambahBarang"/>

		<activity
			android:name=".ControlTutorial"/>

	</application>

</manifest>

